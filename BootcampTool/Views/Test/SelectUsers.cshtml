@model  IEnumerable<BootcampTool.Models.User>

@{
    ViewBag.Title = "SelectUsers";
}

<h2>SelectUsers</h2>

<table id="users">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name </th>
            <th>Last Name </th>
        </tr>
    </thead>
    <tbody>
        @foreach (BootcampTool.Models.User u in Model)
        {
            <tr>
                <td>@u.Id</td>
                <td>@u.Firstname</td>
                <td>@u.LastName</td>
            </tr>
        }
    </tbody>
</table>  